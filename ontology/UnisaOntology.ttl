#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/ont.owl#èCompreso
<http://www.co-ode.org/ontologies/ont.owl#èCompreso> rdf:type owl:ObjectProperty ;
                                                     owl:inverseOf :comprendeInsegnamento ;
                                                     rdfs:domain :Insegnamento ;
                                                     rdfs:range :CorsoDiStudio .


###  http://www.co-ode.org/ontologies/ont.owl#èOffertaDa
<http://www.co-ode.org/ontologies/ont.owl#èOffertaDa> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf :offreOpportunità ;
                                                      rdfs:domain :OpportunitàDiCarriera ;
                                                      rdfs:range :CorsoDiStudio .


###  http://www.semanticweb.org/ontology#comprende
:comprende rdf:type owl:ObjectProperty ;
           rdfs:domain :Università ;
           rdfs:range :Dipartimento ;
           rdfs:comment "Relazione che collega un'Università ai suoi Dipartimenti." ;
           rdfs:label "comprende" .


###  http://www.semanticweb.org/ontology#comprendeInsegnamento
:comprendeInsegnamento rdf:type owl:ObjectProperty ;
                       rdfs:domain :CorsoDiStudio ;
                       rdfs:range :Insegnamento ;
                       rdfs:comment "Relazione che collega un Corso di Studio agli Insegnamenti che comprende." ;
                       rdfs:label "comprendeInsegnamento" .


###  http://www.semanticweb.org/ontology#legatoA
:legatoA rdf:type owl:ObjectProperty ;
         rdfs:domain :Interesse ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :CorsoDiStudio
                                    :Insegnamento
                                  )
                    ] ;
         rdfs:comment "Relazione che collega un Interesse a un Corso di Studio correlato." ;
         rdfs:label "legatoA" .


###  http://www.semanticweb.org/ontology#offreOpportunità
:offreOpportunità rdf:type owl:ObjectProperty ;
                  rdfs:domain :CorsoDiStudio ;
                  rdfs:range :OpportunitàDiCarriera ;
                  rdfs:comment "Relazione che collega un Corso di Studio alle Opportunità di Carriera offerte." ;
                  rdfs:label "offreOpportunità" .


###  http://www.semanticweb.org/ontology#propone
:propone rdf:type owl:ObjectProperty ;
         rdfs:domain :Dipartimento ;
         rdfs:range :CorsoDiStudio ;
         rdfs:comment "Relazione che collega un Dipartimento ai Corsi di Studio che propone." ;
         rdfs:label "propone" .


